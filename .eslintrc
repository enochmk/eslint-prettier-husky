{
	"extends": ["airbnb-base", "prettier"],
	"env": {
		"browser": true,
		"node": true,
		"jest": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"plugins": [],
	"rules": {
		"no-underscore-dangle": "off",
		"no-plusplus": "off",
		"no-unused-vars": [
			"error",
			{
				"vars": "local",
				"args": "none"
			}
		]
	}
}
